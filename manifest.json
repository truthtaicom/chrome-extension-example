{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1",
  "description": "A Chrome extension",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "assets/icon-on.png",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [{
    "matches": ["https://f6wf.com/resource/timesheets/*"],
    "js": [
      "content.js"
    ],
    "css": ["bootstrap.min.css", "app.css"],
    "run_at": "document_idle"
  }]
}